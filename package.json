{
  "name": "catplanner",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 6990",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "update-data": "curl -s 'https://gitlab.com/godfat/battle-cats-rolls/-/raw/master/build/bc-en.yaml?ref_type=heads' | yq -o=json eval > public/data/bc-en.json && bun run src/scraper/grabCatImages.ts",
